<div class="page-Bottom2">
	<div class="container2">
		<div id="right-col" class="rightCol-Index">
		  <%= render "right_col" %>
		</div>
	  <div class="leftCol">
	  	<div class="newsfeed-title">
				<div class="newsfeed-title1 newsfeed-active">
					<%= link_to "All Stream", all_path, :method => :get, :remote => true %>
				</div>
				<div class="newsfeed-title2">
					<%= link_to "Tracks", tracks_path, :method => :get, :remote => true %>
				</div>
				<div class="newsfeed-title2">
					<%= link_to "Posts", posts_path, :method => :get, :remote => true %>
				</div>
				<% if current_user %>
					<div class="newsfeed-title2">
					  <%= link_to "Cliques", cliques_path, :method => :get, :remote => true %>
					</div>
					<div class="newsfeed-title2">
					  <%= link_to "Favorites", favorites_path, :method => :get, :remote => true %>
					</div>
				<% end %>
			</div>
		<div class="newsfeed-subtitle">
			See the latest posts from people you follow
		</div>
			<div id="home-content">
			  <%= render :partial => "all", :locals => { content: @content } %>
			</div>
		</div>
	</div>
</div>
